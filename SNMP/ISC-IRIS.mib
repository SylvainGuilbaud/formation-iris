----------------------------------------------------------------

irisTraps OBJECT IDENTIFIER ::= { iscIRIS 2 }

irisStart NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisStart trap indicates that the IRIS instance
         has been started."
    ::= { irisTraps 1 }

irisStop NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"A irisStop trap indicates that the IRIS instance
         is in the process of shutting down."
    ::= { irisTraps 2 }

irisDBExpand NOTIFICATION-TYPE
    OBJECTS     { irisSysName, irisDBName }
    STATUS      current
    DESCRIPTION
	"An irisDBExpand trap indicates the successful expansion of an
         IRIS database."
    ::= { irisTraps 3 }

irisDBOutOfSpace NOTIFICATION-TYPE
    OBJECTS     { irisSysName, irisDBName }
    STATUS      current
    DESCRIPTION
	"An irisDBOutOfSpace trap indicates that there is a potential
	 problem in the future expansion of an IRIS database. The database
	 is approaching its maximum size or the disk is nearly full."
    ::= { irisTraps 4 }

irisDBStatusChange NOTIFICATION-TYPE
    OBJECTS     { irisSysName, irisDBName, irisDBRead }
    STATUS      current
    DESCRIPTION
	"An irisDBStatusChange trap indicates that the read/write 
         status of an IRIS database has been changed."
    ::= { irisTraps 5 }

irisDBAccessFail NOTIFICATION-TYPE
    OBJECTS     { irisSysName, irisDBName, irisDBError }
    STATUS      current
    DESCRIPTION
	"An irisDBAccessFail trap indicates that an error occurred trying
	 to access an IRIS database, this my be either read or write
	 access. It includes the IRIS error code for the failed access,
	 either DATABASE or DISKHARD."
    ::= { irisTraps 6 }

irisWDStop NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisWriteDemonStop trap indicates that the Write Demon
         of an IRIS instance has stalled."
    ::= { irisTraps 7 }

irisWDPanic NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisWDPanic trap indicates that the Write Demon for an IRIS
         instance has entered 'panic' mode. That is the Write Demon
         is out of buffers and must write database blocks directly to disk
         without first commiting them to the Write Image Journal file."
    ::= { irisTraps 8 }

irisLockTableFull NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisLockTableFull trap indicates that the lock table for
         an IRIS instance is full, causing subsequent Locks to fail."
    ::= { irisTraps 9 }

irisProcessFail NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisProcessFail trap indicates that a process has abnormally
         exited IRIS due to an access violation. Check the console log
         of the instance for further details."
    ::= { irisTraps 10 }

irisECPTroubleDSrv NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisECPTroubleDSrv trap indicates that a connection to this 
	 ECP Data Server for an IRIS database has encountered a serious 
	 communication problem. Check the console log of the instance
	 for further details"
    ::= { irisTraps 11 }

irisECPTroubleASrv NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisECPTroubleASrv trap indicates that a connection from this 
	 ECP Application Server to a remote IRIS database has encountered a serious 
	 communication problem. Check the console log of the instance
	 for further details"
    ::= { irisTraps 12 }

irisAuditLost NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisAuditLost trap indicates that system has failed to record
	 an Audit event. The most likely cause is a problem with space for
	 the Audit database, and requires operator asssitance."
    ::= { irisTraps 13 }

irisLoggedError NOTIFICATION-TYPE
    OBJECTS     { irisSysName, irisSysErrorMsg }
    STATUS      current
    DESCRIPTION
	"An irisLoggedError trap indicates that a 'severe' error has been
	 logged in the IRIS console log. The trap will include the error
	 message as defined in irisSysErrorMsg."
    ::= { irisTraps 14 }

irisLicenseExceed NOTIFICATION-TYPE
    OBJECTS     { irisSysName }
    STATUS      current
    DESCRIPTION
	"An irisLicenseExceed trap indicates that a request for a license 
	 has exceeded the number licenses currently available or allowed."
    ::= { irisTraps 15 }

irisEventLogPost NOTIFICATION-TYPE
    OBJECTS     { irisSysName, irisEvtLogNamespace, irisEvtLogID, irisEvtLogTime, irisEvtLogType, irisEvtLogText, irisEvtLogSource }
    STATUS      current
    DESCRIPTION
	"An entry posted in the Interoperability Event Log."
    ::= { irisTraps 16 }

    irisAppAlert NOTIFICATION-TYPE
    OBJECTS     { irisSysName, irisApp, irisAppSeverity, irisAppText }
    STATUS      current
    DESCRIPTION
	"A generic trap that can be used by IRIS applications to generate alerts
	 via SNMP. See the %Monitor.Alert.External class method for details about
	 how this can be used. It includes the name of the application, a severity
	 code, and a text description of the event."
    ::= { irisTraps 100 }


----------------------------------------------------------------

irisTrapInfo OBJECT IDENTIFIER ::= { iscIRIS 3 }

--
-- IRIS auxiliary information only sent in traps
--

irisDBError OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION
	"The IRIS specific error code for a failed database access.
	 Possible values are:  <DATABASE> or <DISKHARD>"
    ::= { irisTrapInfo  1 }

irisApp OBJECT-TYPE
    SYNTAX          DisplayString(SIZE (20))
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION
	"A brief string which identifies the application which
	 has generated, or source of, a cachAppAlert trap."
    ::= { irisTrapInfo  2 }
    
irisAppSeverity OBJECT-TYPE
    SYNTAX          Integer32 (0..3)
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION
	"A code indicating the severity of the problem for a cachAppAlert trap, 
	 where 0=info, 1=warning, 2=severe, 3=fatal."
    ::= { irisTrapInfo  3 }
    
irisAppText OBJECT-TYPE
    SYNTAX          DisplayString(SIZE (1024))
    MAX-ACCESS      accessible-for-notify
    STATUS          current
    DESCRIPTION
	"The text description of the problem, error or event which caused
	 the cachAppAlert trap to be broadcast."
    ::= { irisTrapInfo  4 }
    



----------------------------------------------------------------

END
