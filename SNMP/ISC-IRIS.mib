--
--  IRIS data is organized into the following groups
--
--	irisSys = a table of general IRIS system information
--	irisPerf = a table of general performance metrics
--	irisDB = a table of IRIS databases
--	irisAppSrv = a table of ECP Application Server metrics
--	irisDataSrv = a table of ECP Data Server metrics
--	irisRes = a table of resource seize metrics
--	irisBuff = a table of global buffer metrics
--	irisWD = a table of Write Demon metrics
--	irisShadowSrc = a table of Shadow Journaling Source connections
--	irisShadowDest = a table of Shadow Destination Servers
--	irisJournal = a table of Journaling status and metrics
--	irisMirror = a table of Mirror Members status and metrics
--	irisProc = a table of top 20 Processes by commands executed
--	irisProcG = a table of top 20 Processes by global references
--	irisProd = a table of Interoperability Productions
--	irisEvtLog = a table of entries in the Interoperability Event Log


--
-- Table of IRIS instances on a system
--

irisSysTab	OBJECT-TYPE
      SYNTAX      SEQUENCE OF IRISSysRow
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
          "The table of IRIS database instances installed on a system."
      ::= { irisObjects 1 }


irisSysRow    OBJECT-TYPE
      SYNTAX      IRISSysRow
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
          "Conceptual row for the table of IRIS instances"
      INDEX  { irisSysIndex }
      ::= { irisSysTab 1 }


IRISSysRow ::=
    SEQUENCE {
    	irisSysIndex		DisplayString,
	irisSysName 		DisplayString,
	irisSysFile     	DisplayString,
	irisSysDir 		DisplayString,
	irisSysVersion 		DisplayString,
	irisSysCurUser 		Counter32,
	irisSysRtnCache 	Integer32,
	irisSysDbCache 		Integer32,
	irisSysErrorMsg		DisplayString,
	irisSysLicenseUsed	Integer32,
	irisSysLicenseHigh	Integer32,
	irisSysState       	DisplayString,
	irisSysLicServerUsed	Integer32,
	irisSysLicServerHigh	Integer32,
	irisSysLicServerAuth	Integer32,
	irisSysLicServerConnectUsed	Integer32,
	irisSysLicServerConnectHigh	Integer32,
	irisSysLicServerError	DisplayString
    }

irisSysIndex     OBJECT-TYPE
    SYNTAX          DisplayString (SIZE (20))
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
	"A unique string index for this IRIS instance, created using the instance name."
    ::= { irisSysRow  1 }

irisSysName     OBJECT-TYPE
    SYNTAX          DisplayString (SIZE (20))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
	"The name of the IRIS instance"
    ::= { irisSysRow  2 }

irisSysFile     OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
	"The name of the IRIS configuration file being used"
    ::= { irisSysRow  3 }

irisSysDir      OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
	"The installation directory for this IRIS instance"
    ::= { irisSysRow  4 }

irisSysVersion  OBJECT-TYPE
    SYNTAX          DisplayString
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
	"The version string for this IRIS instance"
    ::= { irisSysRow  5 }

irisSysCurUser  OBJECT-TYPE
    SYNTAX          Counter32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
	"The current number of users on this instance"
